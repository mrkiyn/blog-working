<table>
    <thead>
        <tr>
            <th>ID</th>    
            <th>Title</th>    
            <th>Body</th>
        </tr> 
    <thead>
    <tbody>
        <% @articles.each do |article| %>
        <tr>
            <td><%= article.id %></td>
            <td><%= article.title %></td>
            <td><%= article.body %></td>
            <td><button><%= link_to "Edit", edit_article_path(article) %></button></td>
            <td><%= button_to "Delete", article, method: :delete %></td>
        </tr>
        <% end %>
    </tbody>   
</table>

<div>
    <%= link_to "New article", new_article_path %>
    
</div>
